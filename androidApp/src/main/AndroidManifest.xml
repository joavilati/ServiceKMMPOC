<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.servicepoc.androidapp">
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>
    <application
            android:allowBackup="true"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <service
            android:name="com.example.servicepoc.androidapp.LocationForegroundService"
            />

        <receiver android:name=".AlarmReceiver" android:exported="true"/>
        <activity android:name="com.example.servicepoc.androidapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>